{
  "name": "portalvj-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest --coverage --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration --coverage --detectOpenHandles --forceExit",
    "lint": "eslint . --ext .js --fix",
    "lint:check": "eslint . --ext .js",
    "build": "echo 'Backend build completed'",
    "docker:build": "docker build -t portalvj-backend .",
    "docker:run": "docker run -p 3000:3000 portalvj-backend",
    "security-audit": "npm audit --audit-level=moderate"
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "pg": "^8.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "supertest": "^6.3.0"
  }
}
